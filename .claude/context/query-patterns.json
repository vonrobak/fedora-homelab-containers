{
  "patterns": [
    {
      "id": "resource_usage_memory_top",
      "description": "Find services using most memory",
      "match": [
        "memory",
        "using",
        "most"
      ],
      "intent": "resource_usage",
      "executor": "get_top_memory_users",
      "cache_key": "top_memory_users",
      "cache_ttl": 300,
      "examples": [
        "What services are using the most memory?",
        "Show me top memory users",
        "Which containers use the most RAM?"
      ]
    },
    {
      "id": "resource_usage_cpu_top",
      "description": "Find services using most CPU",
      "match": [
        "cpu",
        "using",
        "most"
      ],
      "intent": "resource_usage",
      "executor": "get_top_cpu_users",
      "cache_key": "top_cpu_users",
      "cache_ttl": 60,
      "examples": [
        "What's using the most CPU?",
        "Show me top CPU users",
        "Which services use the most CPU?"
      ]
    },
    {
      "id": "service_status_specific",
      "description": "Check if specific service is running",
      "match": [
        "is",
        "{service}",
        "running"
      ],
      "intent": "service_status",
      "executor": "check_service_status",
      "parameters": [
        "service"
      ],
      "examples": [
        "Is jellyfin running?",
        "Is traefik up?",
        "Check status of authelia"
      ]
    },
    {
      "id": "service_status_check",
      "description": "Check service status (alternative pattern)",
      "match": [
        "status",
        "{service}"
      ],
      "intent": "service_status",
      "executor": "check_service_status",
      "parameters": [
        "service"
      ],
      "examples": [
        "Status of jellyfin",
        "Check jellyfin",
        "Jellyfin status"
      ]
    },
    {
      "id": "network_topology_members",
      "description": "List services on a network",
      "match": [
        "network",
        "{network}"
      ],
      "intent": "network_topology",
      "executor": "get_network_members",
      "parameters": [
        "network"
      ],
      "cache_ttl": 600,
      "examples": [
        "What's on the reverse_proxy network?",
        "Show me services on monitoring network",
        "List members of auth_services network"
      ]
    },
    {
      "id": "network_topology_show",
      "description": "Show network members (alternative pattern)",
      "match": [
        "show",
        "{network}",
        "network"
      ],
      "intent": "network_topology",
      "executor": "get_network_members",
      "parameters": [
        "network"
      ],
      "cache_ttl": 600,
      "examples": [
        "Show reverse_proxy network",
        "Show me monitoring network"
      ]
    },
    {
      "id": "disk_usage_summary",
      "description": "Show disk usage for all filesystems",
      "match": [
        "disk",
        "usage"
      ],
      "intent": "resource_usage",
      "executor": "get_disk_usage",
      "cache_key": "disk_usage",
      "cache_ttl": 600,
      "examples": [
        "Show me disk usage",
        "How much disk space is left?",
        "What's the filesystem usage?"
      ]
    },
    {
      "id": "disk_usage_show",
      "description": "Show disk usage (alternative pattern)",
      "match": [
        "show",
        "disk"
      ],
      "intent": "resource_usage",
      "executor": "get_disk_usage",
      "cache_key": "disk_usage_alt",
      "cache_ttl": 600,
      "examples": [
        "Show disk",
        "Show me disk",
        "Disk space"
      ]
    },
    {
      "id": "historical_restarts",
      "description": "Show recent service restarts",
      "match": [
        "recent",
        "restarts"
      ],
      "intent": "historical_events",
      "executor": "get_recent_restarts",
      "cache_key": "recent_restarts",
      "cache_ttl": 300,
      "examples": [
        "Show me recent restarts",
        "Which services restarted recently?",
        "Recent service restarts"
      ]
    },
    {
      "id": "historical_events",
      "description": "Show recent events (alternative pattern)",
      "match": [
        "what",
        "happened"
      ],
      "intent": "historical_events",
      "executor": "get_recent_restarts",
      "cache_key": "recent_events",
      "cache_ttl": 300,
      "examples": [
        "What happened recently?",
        "What happened yesterday?",
        "Show me recent events"
      ]
    },
    {
      "id": "service_config_details",
      "description": "Get configuration for a service",
      "match": [
        "config",
        "{service}"
      ],
      "intent": "configuration",
      "executor": "get_service_config",
      "parameters": [
        "service"
      ],
      "examples": [
        "What's jellyfin's configuration?",
        "Show me prometheus config",
        "Jellyfin config"
      ]
    },
    {
      "id": "service_config_settings",
      "description": "Get service settings (alternative pattern)",
      "match": [
        "settings",
        "{service}"
      ],
      "intent": "configuration",
      "executor": "get_service_config",
      "parameters": [
        "service"
      ],
      "examples": [
        "Jellyfin settings",
        "Show me prometheus settings",
        "What's the memory limit for grafana?"
      ]
    },
    {
      "id": "list_services_all",
      "description": "List all services",
      "match": [
        "list",
        "services"
      ],
      "intent": "service_status",
      "executor": "list_all_services",
      "cache_key": "all_services",
      "cache_ttl": 120,
      "examples": [
        "List all services",
        "Show me all services",
        "What services are running?"
      ]
    },
    {
      "id": "list_services_show",
      "description": "Show all services (alternative pattern)",
      "match": [
        "show",
        "services"
      ],
      "intent": "service_status",
      "executor": "list_all_services",
      "cache_key": "all_services_alt",
      "cache_ttl": 120,
      "examples": [
        "Show services",
        "Show all services"
      ]
    },
    {
      "id": "service_health_all_unhealthy",
      "description": "Find all unhealthy or stopped critical services",
      "match": [
        "unhealthy",
        "services"
      ],
      "intent": "service_health",
      "executor": "get_unhealthy_services",
      "cache_key": "unhealthy_services",
      "cache_ttl": 60,
      "examples": [
        "What services are unhealthy?",
        "Show me stopped services",
        "Which services are down?",
        "What services are not running?"
      ]
    }
  ]
}
